<Steppable Type="DiffusionSolverFE">
   <DiffusionField Name="Nutrient">
      <DiffusionData>
         <FieldName>Nutrient</FieldName>
         <GlobalDiffusionConstant>0.1</GlobalDiffusionConstant>
         <GlobalDecayConstant>0.001</GlobalDecayConstant> 
         <InitialConcentrationExpression>100.0</InitialConcentrationExpression>
      </DiffusionData>
      <SecretionData>
         <Secretion Type="Medium">0.01</Secretion>
      </SecretionData>
